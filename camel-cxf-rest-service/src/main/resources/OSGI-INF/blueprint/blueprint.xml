<?xml version="1.0" encoding="UTF-8"?>
<blueprint
        xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.1.0"
        xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
        http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <bean id="serviceHandler" class="org.apache.servicemix.examples.camel.rest.ServiceHandler" init-method="init" />

    <camelContext xmlns="http://camel.apache.org/schema/blueprint" >
        <route>
            <from uri="activemq:queue:OrderRequestQueue" />
            <log message="OrderRequestConsumerRoute invocation with body: ${body}"/>
            <to uri="direct-vm:insertOrder"/>
        </route>
    </camelContext>

</blueprint>
